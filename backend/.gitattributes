# Track model files with Git LFS
models/saved_model/variables/variables.data-00000-of-00001 filter=lfs diff=lfs merge=lfs -text
models/saved_model/variables/variables.index filter=lfs diff=lfs merge=lfs -text
models/saved_model/saved_model.pb filter=lfs diff=lfs merge=lfs -text
models/saved_model/keras_metadata.pb filter=lfs diff=lfs merge=lfs -text

# Track any large binary files
*.h5 filter=lfs diff=lfs merge=lfs -text
*.keras filter=lfs diff=lfs merge=lfs -text
*.weights filter=lfs diff=lfs merge=lfs -text
*.pb filter=lfs diff=lfs merge=lfs -text
*.ckpt filter=lfs diff=lfs merge=lfs -text
*.data-00000-of-00001 filter=lfs diff=lfs merge=lfs -text